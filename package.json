{
  "name": "catherinepodolski.com",
  "version": "1.0.0",
  "scripts": {
    "start": "parcel src/index.html",
    "build": "rm -rf docs/ && NODE_ENV=production parcel build src/index.html --out-dir docs --public-url https://amandine-perichon.github.io/catherinepodolski.com/ && touch docs/.nojekyll",
    "check": "tsc --noEmit --pretty --skipLibCheck && eslint . --ext .tsx",
    "pretty": "prettier 'src/**' --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amandine-perichon/catherinepodolski.com.git"
  },
  "author": "Amandine Perichon and Nicolas Maquet",
  "dependencies": {
    "@chakra-ui/core": "^0.8.0",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "emotion-theming": "^10.0.27",
    "framer-motion": "^1.11.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-images": "^1.1.7",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@types/react": "^16.9.38",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "autoprefixer": "^9.8.2",
    "eslint": "^7.3.0",
    "eslint-plugin-react": "^7.20.0",
    "parcel-bundler": "^1.12.4",
    "postcss-modules": "^2.0.0",
    "prettier": "^2.0.5",
    "typescript": "^3.9.5",
    "typescript-plugin-css-modules": "^2.3.0"
  }
}